{
    "name": "ufomelkor/cli-tools-installer",
    "type": "project",
    "description": "Installer for various CLI Tools that are useful when developing PHP",
    "keywords": ["CLI", "Tools", "Installer", "Command Line"],
    "homepage": "https://ufomelkor.github.io/cli-tools-installer",
    "authors": [
        {
            "name": "Oskar Pfeifer-Bley",
            "email": "oskar@programming-php.net"
        }
    ],
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "UFOMelkor\\CliTools\\": "src"
        }
    },
    "bin": ["bin/cli-install"],
    "require": {
        "symfony/console": "^3.1",
        "guzzlehttp/guzzle": "^6.2",
        "piwik/ini": "^1.0",
        "twig/twig": "^1.24",
        "cpliakas/git-wrapper": "^1.7",
        "knplabs/github-api": "^2.7",
        "php-http/guzzle6-adapter": "^1.1",
        "alchemy/zippy": "^0.4.8"
    },
    "require-dev": {
        "couscous/couscous": "^1.5"
    }
}
